<?xml version="1.0" encoding="utf-8" ?>
<local:SettingsPageXaml xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SoftwareVets.WorkoutBuilder.Mobile.Views.Pages.SettingsPage"
             xmlns:local="clr-namespace:SoftwareVets.WorkoutBuilder.Mobile.Views.Pages"
             >
    <StackLayout BackgroundColor="{DynamicResource BackgroundColorKey}">
        <Label Text="Settings page" 
               VerticalOptions="CenterAndExpand" 
               HorizontalOptions="CenterAndExpand" 
               TextColor="{DynamicResource TextColorKey}"
               />

        <Label Text="Test" 
               FontSize="Large"
               TextColor="{DynamicResource Test}"
               VerticalOptions="CenterAndExpand" 
               HorizontalOptions="CenterAndExpand" 
               />
    </StackLayout>
</local:SettingsPageXaml>